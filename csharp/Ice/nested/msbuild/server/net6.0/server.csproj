<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>server</AssemblyName>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <OutputPath>../../../</OutputPath>
        <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
        <Company>ZeroC, Inc.</Company>
        <AssemblyTitle>Ice nested demo server</AssemblyTitle>
        <Description>Ice nested demo server</Description>
        <Copyright>Copyright (c) ZeroC, Inc.</Copyright>
        <Product>Ice</Product>
        <AssemblyVersion>1.0.0.0</AssemblyVersion>
        <FileVersion>1.0.0.0</FileVersion>
        <UseAppHost>true</UseAppHost>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(RunConfiguration)' == 'Default' ">
        <StartAction>Project</StartAction>
        <StartWorkingDirectory>..\..\..</StartWorkingDirectory>
        <ExternalConsole>true</ExternalConsole>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="../../../NestedI.cs" />
        <Compile Include="../../../Server.cs" />
        <SliceCompile Include="../../../Nested.ice" />
        <PackageReference Include="zeroc.ice.net" Version="3.7.7" />
        <PackageReference Include="zeroc.icebuilder.msbuild" Version="5.0.7" />
        <Compile Update="generated\Nested.cs">
          <SliceCompileSource>../../../Nested.ice</SliceCompileSource>
        </Compile>
    </ItemGroup>
</Project>
